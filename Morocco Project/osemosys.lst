3203  *------------------------------------------------------------------------
3204  * Salvage value
3205  *------------------------------------------------------------------------
3206   
3207  * Calculates the fraction of the initial capital cost that can be recouped
3208  * at the end of a technologies operational life. The salvage value can be
3209  * calculated using one of two depreciation methods: straight line and
3210  * sinking fund.
3211   
3212  equation SV1_SalvageValueAtEndOfPeriod1(REGION,TECHNOLOGY,YEAR);
3213  SV1_SalvageValueAtEndOfPeriod1(r,t,y)$((DepreciationMethod(r) eq 1) and (((y.val + OperationalLife(r,t)-1) > smax(yy, yy.val)) and (DiscountRate(r) > 0)))..
3214      SalvageValue(r,t,y) =e= CapitalCost(r,t,y)*NewCapacity(r,t,y)*(1-(((1+DiscountRate(r))**(smax(yy, yy.val) - y.val+1) -1) /((1+DiscountRate(r))**OperationalLife(r,t)-1)));
3215   
3216  equation SV2_SalvageValueAtEndOfPeriod2(REGION,TECHNOLOGY,YEAR);
3217  SV2_SalvageValueAtEndOfPeriod2(r,t,y)$((((y.val + OperationalLife(r,t)-1) > smax(yy, yy.val)) and (DiscountRate(r) eq 0)) or ((DepreciationMethod(r) eq 2) and ((y.val + OperationalLife(r,t)-1) > smax(yy, yy.val))))..
3218      SalvageValue(r,t,y) =e= CapitalCost(r,t,y)*NewCapacity(r,t,y)*(1-(smax(yy, yy.val)- y.val+1)/OperationalLife(r,t));
3219   
3220  equation SV3_SalvageValueAtEndOfPeriod3(REGION,TECHNOLOGY,YEAR);
3221  SV3_SalvageValueAtEndOfPeriod3(r,t,y)$(y.val + OperationalLife(r,t)-1 <= smax(yy, yy.val))..
3222      SalvageValue(r,t,y) =e= 0;
3223   
3224  equation SV4_SalvageValueDiscToStartYr(REGION,TECHNOLOGY,YEAR);
3225  SV4_SalvageValueDiscToStartYr(r,t,y)..
3226      DiscountedSalvageValue(r,t,y) =e= SalvageValue(r,t,y)/((1+DiscountRate(r))**(1+smax(yy, yy.val) - smin(yy, yy.val)));
