3068  equation CAb1_PlannedMaintenance(REGION,TECHNOLOGY,YEAR);
3069  CAb1_PlannedMaintenance(r,t,y)..
3070      sum(l, RateOfTotalActivity(r,l,t,y)*YearSplit(l,y)) =l= sum(l,TotalCapacityAnnual(r,t,y)*CapacityFactor(r,t,l,y)*YearSplit(l,y))*AvailabilityFactor(r,t,y)*CapacityToActivityUnit(r,t);
